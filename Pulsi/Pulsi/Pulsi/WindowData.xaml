<Window x:Class="Pulsi.WindowData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WindowData" Height="800" Width="1280">
    <Grid>
        <Image x:Name="imgLogo" HorizontalAlignment="Left" Height="81" Margin="402,89,0,0" VerticalAlignment="Top" Width="500" Source="Logo.png"/>
        <ComboBox x:Name="comboxBaby" HorizontalAlignment="Left" Margin="402,242,0,0" VerticalAlignment="Top" Width="130" Height="30" SelectionChanged="comboxBaby_SelectionChanged"/>
        <Label x:Name="lblBaby" Content="Baby" HorizontalAlignment="Left" Margin="402,207,0,0" VerticalAlignment="Top" Width="130" Height="30" FontFamily="Arial" FontSize="16"/>
        <Button x:Name="btnLogOut" Content="uitloggen" HorizontalAlignment="Left" Margin="772,242,0,0" VerticalAlignment="Top" Width="130" Click="btnLogOut_Click" Height="30" FontFamily="Arial" FontSize="16"/>
        <DataGrid x:Name="datagridBaby" HorizontalAlignment="Left" Margin="402,318,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Width="500" Height="212" FontFamily="Arial"/>

    </Grid>
</Window>
